{
  "name": "leaction",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && NODE_ENV=production vite build",
    "check": "tsc --noEmit",
    "serve": "(cd functions/ && tsc) && firebase serve",
    "test": "jest",
    "format": "eslint --fix src/**/*.{ts,vue}",
    "emulate": "yarn build_function_for_emulator && yarn build_for_emulator && firebase emulators:start",
    "build_for_emulator": "tsc --noEmit && vite build --mode emulator",
    "build_function_for_emulator": "(cd functions/ && npm run build)",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-2",
    "axios": "^0.26.1",
    "dayjs": "^1.10.7",
    "firebase": "^9.6.3",
    "js-sha256": "^0.9.0",
    "nanoid": "^3.1.32",
    "sweetalert2": "^11.3.5",
    "vue": "^3.2.26",
    "vue-gtag-next": "^1.14.0",
    "vue-router": "^4.0.12",
    "vuedraggable": "^4.0.1",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/xss-filters": "^0.0.27",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "firebase-tools": "^10.1.0",
    "jest": "^27.4.7",
    "postcss": "^8.4.5",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^2.5.1",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint-config-recommended": "^7.0.0",
    "tailwindcss": "^2.0.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.0.3",
    "vite": "^2.7.12"
  },
  "eslintConfig": {
    "ignorePatterns": "functions/**"
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
}
